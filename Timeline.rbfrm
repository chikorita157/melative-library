lic          =   ""      Left            =   208      LockBottom      =   True      LockedInPosition=   False      LockLeft        =   False      LockRight       =   True      LockTop         =   False      Scope           =   0      TabIndex        =   10      TabPanelIndex   =   0      TabStop         =   True      TextFont        =   "System"      TextSize        =   10      TextUnit        =   0      Top             =   311      Underline       =   ""      Visible         =   False      Width           =   82   EndEnd#tag EndWindow#tag WindowCode	#tag Event		Sub Activate()		  KeyboardUIMode.SetControls(self,KeyboardUIMode.GetMode)		End Sub	#tag EndEvent	#tag Event		Sub Close()		  f.Delete		  window1.streamwindow = false		End Sub	#tag EndEvent	#tag Event		Sub Open()		  window1.streamwindow = true		  loadtimeline		End Sub	#tag EndEvent	#tag MenuHandler		Function FileClose() As Boolean Handles FileClose.Action			me.close			Return True					End Function	#tag EndMenuHandler	#tag Method, Flags = &h0		Sub loadtimeline()		  dim socket1 as new httpsocket		  dim listdata, m, u, reply, replyname,updatediv as string		  dim d as new dictionary		  dim d2 as new dictionary		  dim p as new xmlnodelist		  dim p2 as new xmlnodelist		  dim c , i as integer		  dim r as xmlnode		  socket1.yield = true		  socket1.setrequestheader "Authorization","Basic " + app.loginenc		  listdata= socket1.get("http://melative.com/api/statuses/friends_timeline.xml?page=" + cstr(page) ,10)		  If socket1.httpstatuscode = 200 then		    // Output the temp HTML		    dim stream as string		    Dim t as TextOutputStream		    f = SpecialFolder.Temporary		    f = f.child("t.html")		    t=TextOutputStream.Create(f)		    dim XML as new XMLDocument		    xml.LoadXML(listdata)		    r = getXMLroot(xml)		    p = getanything(r, "//statuses/status")		    p2 = getanything(r, "//statuses/status/user")		    replydict = new dictionary		    c = p.Length - 1		    for i = 0 to c		      d = parseAnime(p.Item(i))		      d2 = parseAnime(p2.Item(i))		      If instr(d.value("text"), "@" + app.memusername) > 0 then		        updatediv = "<div class='updatereply'>"		      else		        updatediv = "<div class='update'>"		      end if		      If app.memusername = d2.value("screen_name")  then		        reply = ""		      else		        reply = "<br /><a href='ml:reply=" + d.value("id") + "'>reply</a>"		      end if		      If d.value("in_reply_to_screen_name") = "" then		        replyname = ""		      else		        replyname = " in reply to <a href='http://melative.com/" +  d.value("in_reply_to_screen_name") + "/'>"  + d.value("in_reply_to_screen_name") + "</a>"		      end if		      u = "<a href='http://melative.com/" + d2.value("screen_name") +"'>"		      m = updatediv +"<div id='userimage'>"+ u + "<img src='" + d2.value("profile_image_url") + "?size=48' alt='' /></a></div><div class='content'><b>" + u + d2.value("name") + "</a></b> " + phasemessage(d.value("text")) + "<br /><small>"+ d.value("source") + " on " + d.value("created_at") + replyname +reply +"</small></div></div>" + endofline		      // Pagination		      If i=c then		        If page = 1 then		          m = m + "<div class='content'><a href='ml:page=" + cstr(page + 1) + "'>Next &gt;</a></div>"		        elseif page > 1 then		          m = m + "<div class='content'><a href='ml:page=" + cstr(page - 1) + "'>&lt; Previous</a> | <a href='ml:page=" + cstr(page + 1) + "'>Next &gt;</a></div>"		        end if		      end if		      //Apply to stream		      stream = stream + m		      replydict.value(d.value("id")) = d2.value("screen_name")		      		    next		    t.write htmHeader + stream + "</body>" + endofline + "</html>"		    t.close		    htmlviewer1.loadpage(f)		  end if		  //Reset Timer		  timer1.Reset		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub melpostupdate()		  dim socket1 as new httpsocket		  pushbutton1.enabled = false		  dim data as string		  		  // create and populate the form object		  dim form2 as New Dictionary		  // Set content		  		  // Use Normal Updating		  If replyid = "" then		    form2.value("message") = ReplaceLineEndings(commentsfield.text, EndOfLine.UNIX)		  else		    commentsfield.text = "@" + replydict.value(replyid) + " " + commentsfield.text		    form2.value("message") = ReplaceLineEndings(commentsfield.text, EndOfLine.UNIX)		    form2.value("in_reply_to_status_id") = replyid		  end if		  // Set Client ( e.g. <username> in anime <time> ago from MAL Client OS X)		  form2.value("source") = "Melative Library"		  // setup the socket to POST the form		  socket1.setFormData form2		  socket1.setrequestheader "Authorization","Basic " + app.loginenc		  data = socket1.post("http://melative.com/api/micro/update.json",10)		  		  // release data from memory		  form2 = nil		  If socket1.httpstatuscode = 200 then		    commentsfield.text = ""		    pushbutton1.enabled = true		    If cancelreply.visible = true then		      cancelreply.visible = false		      replyid = ""		      replyto.text = ""		    end if		    loadtimeline		  else		    // Show Error		    self.errboxshow("Melative Library was unable to post your update on Melative",   "Check to see if your internet connection is working or revertify your Melative account information and try posting your update again.")		    pushbutton1.enabled = true		  end if		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function phasemessage(text as string) As string		  Dim rg as New RegEx // Regex action		  rg.options.ReplaceAllMatches = true		  // Phase URLs		  rg.SearchPattern = "http://([\w+?\.\w+])+([a-zA-Z0-9\~\!\@\#\$\%\^\&amp;\*\(\)_\-\=\+\\\/\?\.\:\;\'\,]*)?"		  rg.ReplacementPattern = "<a href='$&'>$&</a>"		  text = rg.replace(text)		  // @ Reply Phasing		  rg.searchpattern = "@([a-zA-Z0-9\-_][a-zA-Z0-9\-_][a-zA-Z0-9\-_]+)"		  rg.ReplacementPattern = "@<a href='http://melative.com/\1'>\1</a>"		  text = rg.replace(text)		  // Return the string		  return text		exception err as RegExException		  return text		End Function	#tag EndMethod	#tag Property, Flags = &h0		f As folderitem	#tag EndProperty	#tag Property, Flags = &h0		page As Integer = 1	#tag EndProperty	#tag Property, Flags = &h0		replydict As dictionary	#tag EndProperty	#tag Property, Flags = &h0		replyid As string	#tag EndProperty	#tag Constant, Name = htmHeader, Type = String, Dynamic = False, Default = \"<html>\r<head>\r<style type\x3D\"text/css\">\ra:link {\rcolor: #338ec2;\rtext-decoration: none; \r}\ra:visited {\rcolor: #338ec2; \rtext-decoration: none; \r}\ra:hover {\rcolor: orange; \rtext-decoration: underline; \r}\ra:active {\rcolor: blue; \r}\rbody {\r\tfont-family: \"Segoe UI\"\x2C\"Lucida Grande\"\x2C Arial\x2C serif;\r\tfont-size: 14px;\r}\r#userimage {\r height: auto; \r width:48px; \r float:left; \r vertical-align:middle; \r}\r#userimage img {\rheight: 48px;\rwidth: 48px;\rborder: 0;\r}\r \r.content {\r margin-left: 58px;\r}\r\r.update {\r margin:5px;\r clear:left;\r padding:5px 0;\r}\r.updatereply {\r margin 5px;\r clear:left;\r padding:5px 0;\rbackground-color: #E8EEFF;\rmin-height: 50px;\r}\r</style>\r<meta http-equiv\x3D\"Content-Type\" content\x3D\"text/html; charset\x3DUTF-8\" />\r</head>\r<body>\r", Scope = Public	#tag EndConstant#tag EndWindowCode#tag Events commentsfield	#tag Event		Sub TextChange()		  MessageLength.text = cstr(1000-Len(commentsfield.text))		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton1	#tag Event		Sub Action()		  		  if commentsfield.text = "" then		    self.errboxshow("Melative Library was unable to post the update.", "You cannot leave the message field blank. You must have a message written.")		  else		    If val(MessageLength.text) < 0 then		      		      Dim d as New MessageDialog //declare the MessageDialog object		      Dim b as MessageDialogButton //for handling the result		      d.icon=MessageDialog.GraphicCaution //display warning icon		      d.ActionButton.Caption="Post"		      d.CancelButton.Visible=True    //show the Cancel button		      d.Message="Your message is over the character limit. Are you sure you want to post this update?"		      d.Explanation="If you continue, part of your message will be cut off to fit within the character limit."		      b=d.ShowModalWithin(self.TrueWindow)//display the dialog		      Select Case b //determine which button was pressed.		      Case d.ActionButton		        //user pressed Delete		        melpostupdate		      Case d.CancelButton		        //user pressed Cancel		      End select		    else		      melpostupdate		    end if		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events Timer1	#tag Event		Sub Action()		  loadtimeline		End Sub	#tag EndEvent#tag EndEvents#tag Events HTMLViewer1	#tag Event		Function CancelLoad(URL as String) As Boolean		  If url = ReplaceAll(f.URLPath, "/localhost/", "//")  then		    return false		  elseif instr(url, "ml:reply=") > 0 then		    replyid = replace(url, "ml:reply=", "")		    replyto.text = "Replying to " + replydict.value(replyid)		    cancelreply.visible = true		    return true		  elseif instr(url, "ml:page=") > 0 then		    page = val(replace(url, "ml:page=", ""))		    loadtimeline		    return true		  elseif instr(url, "/anime/") > 0 then		    ShowMoreInfoWindow(DecodeURLComponent(replace(url, "http://melative.com/anime/", ""),Encodings.UTF8), "anime")		    return true		  elseif instr(url, "/manga/") > 0 then		    ShowMoreInfoWindow(DecodeURLComponent(replace(url, "http://melative.com/manga/", ""),Encodings.UTF8), "manga")		    return true		  elseif instr(url, "/lightnovel/") > 0 then		    ShowMoreInfoWindow(DecodeURLComponent(replace(url, "http://melative.com/lightnovel/", ""),Encodings.UTF8), "lightnovel")		    return true		  elseif instr(url, "/vn/") > 0 then		    ShowMoreInfoWindow(DecodeURLComponent(replace(url, "http://melative.com/vn/", ""),Encodings.UTF8), "vn")		    return true		  elseif instr(url, "/adrama/") > 0 then		    ShowMoreInfoWindow(DecodeURLComponent(replace(url, "http://melative.com/adramal/", ""),Encodings.UTF8), "adrama")		    return true		  else		    showurl url		    return true		  end if		End Function	#tag EndEvent#tag EndEvents#tag Events BevelButton1	#tag Event		Sub Action()		  loadtimeline		End Sub	#tag EndEvent#tag EndEvents#tag Events smallbut	#tag Event		Sub Action()		  commentsfield.text = commentsfield.text + "<small></small>"		  commentsfield.selstart = commentsfield.text.Len		End Sub	#tag EndEvent#tag EndEvents#tag Events ItalicBut	#tag Event		Sub Action()		  commentsfield.text = commentsfield.text + "<i></i>"		  commentsfield.selstart = commentsfield.text.Len		End Sub	#tag EndEvent#tag EndEvents#tag Events BoldBut	#tag Event		Sub Action()		  commentsfield.text = commentsfield.text + "<b></b>"		  commentsfield.selstart = commentsfield.text.Len		End Sub	#tag EndEvent#tag EndEvents#tag Events delbut	#tag Event		Sub Action()		  commentsfield.text = commentsfield.text + "<del></del>"		  commentsfield.selstart = commentsfield.text.Len		End Sub	#tag EndEvent#tag EndEvents#tag Events bigbut	#tag Event		Sub Action()		  commentsfield.text = commentsfield.text + "<big></big>"		  commentsfield.selstart = commentsfield.text.Len		End Sub	#tag EndEvent#tag EndEvents#tag Events cancelreply	#tag Event		Sub Action()		  replyto.text = ""		  replyid = ""		  me.visible = false		End Sub	#tag EndEvent#tag EndEvents